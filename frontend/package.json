{
  "name": "@everydle/frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "generate": "encore gen client everydle-qnxi -e staging --output=./lib/client.ts",
    "ios": "next build --turbopack && npx cap sync && npx cap open ios",
    "mobile": "next build --turbopack && bun x cap sync && bun run apk && bun run open-apk",
    "store": "next build --turbopack && bun x cap sync && bun run apk && bun run open-aab",
    "apk": "cd android && cmd /c gradlew.bat assembleDebug && cmd /c gradlew.bat bundleRelease && cd ..",
    "open-apk": "powershell -Command \"ii android/app/build/outputs/apk/debug\"",
    "aab": "cd android && cmd /c gradlew.bat bundleRelease && cd ..",
    "open-aab": "powershell -Command \"ii android/app/build/outputs/bundle/release\"",
    "clean": "cd android && cmd /c gradlew.bat clean"
  },
  "dependencies": {
    "@capacitor/android": "^8.0.0",
    "@capacitor/app": "^8.0.0",
    "@capacitor/cli": "^8.0.0",
    "@capacitor/core": "^8.0.0",
    "@capacitor/ios": "^8.0.0",
    "@capacitor/push-notifications": "^8.0.0",
    "@capacitor/status-bar": "^8.0.0",
    "@heroicons/react": "^2.2.0",
    "@tanstack/react-query": "^5.90.12",
    "convex": "^1.31.2",
    "firebase": "^12.7.0",
    "lucide-react": "^0.554.0",
    "next": "16.0.10",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
